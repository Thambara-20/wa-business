{
  "compilerOptions": {
    "lib": ["es5", "es6", "es2020"],
    "target": "es2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./dist",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true
  }
}
